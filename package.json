  "module": "dist/graphql-defs.modern.js",
  "main": "dist/graphql-defs.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^3.9.0",
